{
    "builds": [{
        "src": "management/wsgi.py",
        "use": "@natanffs/vercel-python-wsgi",
        "config": { "maxLambdaSize": "15mb" ,"runtime": "python3.9"},
        "output": "/(.*)"
        
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ]
    
}